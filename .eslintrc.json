{
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "extends": [
    "react-app"
  ],
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "comma-dangle": [
      "error",
      "never"
    ],
    "@typescript-eslint/no-unused-vars": [
      "error"
    ],
    "no-console": [
      "warn"
    ]
  }
}
